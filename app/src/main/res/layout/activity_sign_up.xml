<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.linux.muscleapp.SignUpActivity"
    android:layout_margin="@dimen/activity_margin">
    <TextView
        android:id="@+id/txvSignUpTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/margin_titlelogin"
        android:text="@string/sign_up"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium" />
    <ImageView
        android:id="@+id/imgSignUpDate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/txvSignUpTitle"
        android:src="@mipmap/ic_datepicker"/>
    <TextView
        android:id="@+id/txvRes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/imgSignUpDate"
        android:text="por defecto"/>

</RelativeLayout>
